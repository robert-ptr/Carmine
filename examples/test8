enum
{
    MAX = 10,
    MIN = 2,
    MEDIUM = 5,
};

const val = null;

module xor10(a, b) -> out
{
    const i = 0;
    while i < 10
    {
        out = and(or(a, b), not(and(a, b)));
        i = i + 1;
    }
}

module xorc(a, b, c) -> out
{
    if 3 + 4 > 5
        out = and(or(a, b), not(and(a, b)));
}

module xor1(a, b) -> out
{
    out = and( or(a, b), not(and(a, b)));
}